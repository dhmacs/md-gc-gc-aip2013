<section class = content>
  <h1>Our Rooms</h1>



  <% @our_rooms.each do |room| %>
      <div class="product_item">
        <div class="img_container"><%= image_tag(room.image.complete_name, :class => 'thumbnail') %></div>
        <h4> <%= link_to room.name, {:controller => "products", :action => 'our_categories_by_room', :room_id => room.id} %></h4>
      </div>
  <% end %>
  <div class = group_links>
  <%= link_to 'Back to Products', products_path %>
  </div>
</section>


