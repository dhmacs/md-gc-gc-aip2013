<section class="content">
  <h1>All designers</h1>
  <% @designers.each do |designer| %>
      <div class="designer_item">
        <div><%= image_tag(designer.image.complete_name, :class => 'designer_thumbnail') %></div>
        <h4><%= link_to designer.full_name, designer %></h4>
      </div>
  <% end %>
</section>

